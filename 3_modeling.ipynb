   "source": [
    "# Apply embeddings to the reviews \n",
    "embedding_model = SentenceTransformer(\"BAAI/bge-base-en-v1.5\") # A transformer-based model optimized for semantic search \n",
    "embeddings = embedding_model.encode(all_reviews_df, show_progress_bar=True)"
   ]
