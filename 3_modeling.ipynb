   "source": [
    "# Extracts intent and text_summary columns and format each row into one single string\n",
    "all_reviews_df = [f\"{review['intent']}: {review['text_summary']}\" for _, review in all_reviews.iterrows()]\n",
    "print(all_reviews_df[:5])"
   ]
